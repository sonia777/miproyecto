<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ProyectoGym</display-name>
  
  <servlet>
    <servlet-name>WelcomeJSP</servlet-name>
    <jsp-file>/Index.jsp</jsp-file>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>WelcomeJSP</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>vista.LoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
    <servlet-name>FormularyServlet</servlet-name>
    <servlet-class>vista.FormularyServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>FormularyServlet</servlet-name>
    <url-pattern>/Formulary</url-pattern>
  </servlet-mapping>
  
  
  
    
  <servlet>
    <servlet-name>InscripcionServlet</servlet-name>
    <servlet-class>vista.InscripcionServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>InscripcionServlet</servlet-name>
    <url-pattern>/Inscripcion</url-pattern>
  </servlet-mapping>
  
 
  
  

</web-app>